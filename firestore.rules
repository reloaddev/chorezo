rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.auth != null &&
                         request.auth.uid in [
                           "dFNLhIRlojMbBvyGoNQo3XXoD9t2",
                           "rwZnBP5Mv9T2DAmb5P8yejtfbUg2"
                         ];
    }
  }
}
